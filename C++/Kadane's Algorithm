#include<bits/stdc++.h>
using namespace std;

 int maxsum(int A[], int n)
 {
        int ans=INT_MIN;
    
    for(int i=0;i<n;i++)
    {
         int sum=0;
         int mx=INT_MIN;
        for(int j=i;j<n-i;j++)
        {

            sum+=A[j];
            mx=max(sum,mx);
            if(mx>ans)
            {
                ans=mx;
            
            }
            
            
        }
        
    }
    return ans;
        
    }
    

   
int main()
{
    int n; cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    { 
        cin>>arr[i];
    }
    
    
    cout<<maxsum(arr,n);
    
     
    


 return 0;

}
//Time Complexity: O(N)
//Auxiliary Space: O(1)
